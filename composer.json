{
  "name": "d3vnz/issuetracker",
  "type": "library",
  "description": "GitHub issue tracking integration for Laravel applications using Filament 3",
  "keywords": ["laravel", "filament", "github", "issue-tracking"],
  "homepage": "https://github.com/d3vnz/issuetracker",
  "license": "proprietary",
  "authors": [
    {
      "name": "D3V Services Limited",
      "email": "info@d3v.nz",
      "homepage": "https://d3v.nz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "filament/filament": "^3.0",
    "graham-campbell/github": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "D3vnz\\IssueTracker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "D3vnz\\IssueTracker\\Providers\\IssueTrackerServiceProvider"
      ],
      "migrations": [
        "database/migrations"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}